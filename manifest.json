{
    "manifest_version": 3,
    "name": "Tabgether",
    "description": "Open your browser tabs with one click",
    "version": "1.0.0",
    "author": "Jonathan Hermam",
    "action": {
        "default_title": "Tabgether",
        "default_popup": "pages/index.html",
        "default_icon": {
            "16": "images/icon-16.png",
            "128": "images/icon-128.png"
        }
    },
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "permissions": [
        "storage",
        "tabs"
    ]
}